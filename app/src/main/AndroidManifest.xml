<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:enableOnBackInvokedCallback="true"
        android:fullBackupContent="@xml/backup_rules"
        android:networkSecurityConfig="@xml/network_security_config"
        android:icon="@drawable/applogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/applogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Jobber"
        tools:targetApi="31">
        <activity
            android:name=".Activity.SignUser.SignInActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.Calendrier.DetailsEvenementActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.Candidature.StateChangeActivity"
            android:exported="false"
            android:parentActivityName=".Activity.Candidature.DetailsCandidatureActivity"
            android:theme="@style/Theme.AppCompat.Dialog" />

        <receiver android:name=".Notification.NotificationReceiver" />

        <activity
            android:name=".Activity.Relance.EditRelanceActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Activity.Entretien.EditEntretienActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Activity.Entreprise.EditEntrepriseActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Activity.Contact.EditContactActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".Activity.Appel.EditAppelActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Activity.Candidature.EditCandidatureActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Activity.Relance.DetailsRelanceActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Activity.Relance.AddRelanceActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".Activity.Entretien.DetailsEntretienActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".Activity.Entretien.AddEntretienActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".Activity.Appel.DetailsAppelActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat">
            <meta-data
                android:name="android.support.PAREN_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".Activity.Appel.AddAppelActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".Activity.Entreprise.DetailsEntrepriseActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat" />

        <meta-data
            android:name="android.support.PAREN_ACTIVITY"
            android:value=".MainActivity" />

        <activity
            android:name=".Activity.Contact.DetailsContactActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat">
            <meta-data
                android:name="android.support.PAREN_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".Activity.Contact.AddContactActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".Activity.Candidature.DetailsCandidatureActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".Activity.Candidature.AddCandidatureActivity"
            android:exported="true"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".Activity.WelcomeActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".Activity.SignUser.LoginActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Jobber">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>